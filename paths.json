{
  "compilerOptions": {
    "esModuleInterop": true,
    "paths": {
      "@app/*": [
        "src/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@store": [
        "src/store/"
      ],
      "@components": [
        "src/components/index"
      ],
      "@modules/*": [
        "src/modules/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ]
    }
  }
}